    "graphql": "15.5.1",
    "graphql-introspection-json-to-sdl": "1.0.3"
  },
  "scripts": {
    "sdl": "graphql-introspection-json-to-sdl graphql.schema.json > schema.graphql",
    "generate": "node generate.js",
