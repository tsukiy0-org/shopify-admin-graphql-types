  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@packages/config": "*",
    "@packages/tsconfig": "*",
    "@graphql-codegen/cli": "2.0.1",
